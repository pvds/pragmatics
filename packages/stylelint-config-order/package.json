{
  "name": "stylelint-config-pragmatic-order",
  "version": "1.4.0",
  "description": "Opinionated stylelint config pragmatically ordering css properties",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pvds/stylelint-config-pragmatic-order.git"
  },
  "author": "pvds <pvdsteen@gmail.com>",
  "license": "ISC",
  "homepage": "https://www.npmjs.com/package/stylelint-config-pragmatic-order",
  "bugs": {
    "url": "https://github.com/pvds/stylelint-config-pragmatic-order/issues"
  },
  "keywords": [
    "stylelint",
    "stylelint-order",
    "stylelint-config",
    "stylelint-plugin",
    "css order",
    "css grouping",
    "property order",
    "property grouping",
    "declaration order",
    "declaration grouping",
    "pragmatic order",
    "pragmatic grouping"
  ],
  "main": "index.js",
  "files": [
    "CODE_OF_CONDUCT.md",
    "CONTRIBUTING.md"
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint --fix .",
    "format": "prettier --write **/*.{js,md}"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --fix",
    "*.{js,md}": "prettier --write"
  },
  "devDependencies": {
    "eslint": "^7.8.1",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^24.0.0",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "lint-staged": "^10.3.0",
    "prettier": "^2.1.1",
    "stylelint": "^13.7.0"
  },
  "peerDependencies": {
    "stylelint": ">=13"
  },
  "dependencies": {
    "stylelint-order": "^4.1.0"
  }
}
